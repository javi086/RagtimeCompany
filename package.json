    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "start": "concurrently \"npm run watch\" \"npm run dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/javi086/RagtimeCompany.git"
  },
